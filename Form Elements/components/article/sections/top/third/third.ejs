<div class="section-column third">
    <!-- range_slider -->
    <div class="element-group width-medium indent-large">
        <div class="element-group__description">
            <label for="range_slider">Range Slider</label>
            <span>5 000₽ - 10 000₽</span>
        </div>
        <div id="range_slider" class="input-group column">
            <div class="form-element range-slider">
                <div class="range-slider__full-bar">
                    <div class="range-slider__active-bar-wrapper">
                        <div class="range-slider__active-bar">
                            <div class="range-slider__start-point"></div>
                            <div class="range-slider__end-point-wrapper">
                                <div class="range-slider__end-point"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- buttons -->
    <div class="element-group width-large indent-large">
        <div class="element-group__description">
            <label for="buttons">Buttons</label>
        </div>
        <div id="buttons" class="input-group column">
            <div class="row">
                <div class="form-element button">Click Me</div>
                <div class="form-element button disabled">Click Me</div>
            </div>
            <div class="row indent-medium">
                <div class="form-element button bordered">
                    <div class="button__container">Click Me</div>
                </div>
                <div class="form-element button bordered disabled">
                    <div class="button__container">Click Me</div>
                </div>
            </div>
            <div class="row indent-medium">
                <div class="form-element button inline">Click Me</div>
                <div class="form-element button inline disabled">Click Me</div>
            </div>
            <div class="row indent-medium">
                <div class="form-element button submit">
                    <span>Перейти к оплате</span>
                    <div class="button__icon clickable">arrow_forward</div>
                </div>
            </div>
        </div>
    </div>
    <!-- pagination -->
    <div class="element-group width-large indent-large">
        <div class="element-group__description">
            <label for="pagination">Pagination</label>
        </div>
        <div id="pagination" class="input-group row">
            <div class="form-element pagination column">
                <div class="pagination__buttons row">
                    <div class="pagination__button active">1</div>
                    <div class="pagination__button">2</div>
                    <div class="pagination__button">3</div>
                    <div class="pagination__button">...</div>
                    <div class="pagination__button">15</div>
                    <div class="pagination__button next">arrow_forward</div>
                </div>
                <div class="pagination__afterword indent-tiny">1 – 12 из 100+ вариантов аренды</div>
            </div>
        </div>
    </div>
</div>